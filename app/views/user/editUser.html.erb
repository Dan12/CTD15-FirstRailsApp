<h1 class = "editUser">Edit an Existing user</h1>
<form action = "/submitEditUser/<%=@user.id%>">
  <div class="form-group">
    <label for="exampleInputEmail1">Name</label>
    <input type="text" name = "name" class="form-control name" placeholder="name" value = "<%=@user.name%>">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Image Url</label>
    <input type="text" name = "imgUrl" class="form-control imgUrl" placeholder="http://" value = "<%=@user.imgUrl%>">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">membSince</label>
    <input type="text" name = "membSince" class="form-control membSince" placeholder="1990" value = "<%=@user.membSince%>">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Location</label>
    <input type="text" name = "location" class="form-control location" placeholder="Brooklyn" value = "<%=@user.location%>">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Completion</label>
    <input type="text" name = "completion" class="form-control completion" placeholder="50" value = "<%=@user.completion%>">
  </div>
  <input type = "submit" class="btn btn-default">
</form>